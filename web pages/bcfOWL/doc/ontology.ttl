@base <http://lbd.arch.rwth-aachen.de/bcfOWL/> .

<http://lbd.arch.rwth-aachen.de/bcfOWL/> rdf:type owl:Ontology ;
                                          owl:versionIRI <http://lbd.arch.rwth-aachen.de/bcfOWL/> ;
                                          <http://purl.org/dc/terms/creator> "Jakob Beetz" ,
                                                                             "Jyrki Oraskai" ,
                                                                             "Oliver Schulz" ;
                                          <http://purl.org/dc/terms/description> "BCF 2.1 Ontology Description based on https://github.com/buildingSMART/BCF-XML" ;
                                          <http://purl.org/dc/terms/license> "https://creativecommons.org/licenses/by/4.0/" ;
                                          <http://purl.org/dc/terms/title> "BIM collaboration Format Ontology" ;
                                          <http://purl.org/vocab/vann/prefferedNamespacePrefix> "bcfOWL" ;
                                          <http://purl.org/vocab/vann/prefferedNamespaceURI> "http://lbd.arch.rwth-aachen.de/bcfOWL/" ;
                                          owl:versionInfo "0.7.0" .

